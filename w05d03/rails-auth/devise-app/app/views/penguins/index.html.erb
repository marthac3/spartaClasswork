<p id="notice"><%= notice %></p>

<h1>Penguins</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Height</th>
      <th>Weight</th>
      <th>Color</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @penguins.each do |penguin| %>
      <tr>
        <td><%= penguin.name %></td>
        <td><%= penguin.height %></td>
        <td><%= penguin.weight %></td>
        <td><%= penguin.color %></td>
        <td><%= link_to 'Show', penguin %></td>
        <td><%= link_to 'Edit', edit_penguin_path(penguin) %></td>
        <td><%= link_to 'Destroy', penguin, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Penguin', new_penguin_path %>
